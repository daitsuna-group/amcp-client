{
    "name": "daitsuna-group/amcp-client",
    "description": "AMCP Client for PHP",
    "type": "library",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Daitsuna\\AmcpClient\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Daitsuna Group",
            "email": "info@daitsuna.com"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Daitsuna\\AmcpClient\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}